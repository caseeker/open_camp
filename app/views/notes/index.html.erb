<h1>Listing notes</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Body</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @notes.each do |note| %>
  <tr>
    <td><%= note.title %></td>
    <td><%= note.body %></td>
    <td><%= link_to 'Show', note_path(note) %></td>
    <td><%= link_to 'Edit', edit_note_path(note) %></td>
    <td></td>
  </tr>
<% end %>
</table>

